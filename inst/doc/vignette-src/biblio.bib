	Journal = {Clin Cancer Res},
	Number = {13},
	Pages = {4103-4110},
	Title = {{Molecular Subtypes of Breast Cancers Detected in Mammography Screening and Outside of Screening}},
	Url = {http://clincancerres.aacrjournals.org/cgi/content/abstract/14/13/4103},
	Volume = {14},
