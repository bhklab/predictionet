	Doi = {10.1093/jnci/djj329},
	Eprint = {http://jnci.oxfordjournals.org/cgi/reprint/jnci;98/17/1183.pdf},
	Journal = {J. Natl. Cancer Inst.},
	Number = {17},
	Pages = {1183-1192},
	Title = {{Validation and Clinical Utility of a 70-Gene Prognostic Signature for Women With Node-Negative Breast Cancer}},
	Url = {http://jnci.oxfordjournals.org/cgi/content/abstract/jnci;98/17/1183},
