	Journal = {Cancer Res},
	Journal-Full = {Cancer research},
	Mesh = {Breast Neoplasms; Carcinoma, Ductal, Breast; Carcinoma, Lobular; Diagnosis, Differential; Female; Gene Expression Profiling; Humans; Multigene Family; Oligonucleotide Array Sequence Analysis; Polymerase Chain Reaction},
	Month = {Nov},
	Number = {21},
	Pages = {7167-75},
	Pmid = {14612510},
	Pst = {ppublish},
	Title = {Differentiation of lobular versus ductal breast carcinomas by expression microarray analysis},
	Volume = {63},
	Year = {2003}}

