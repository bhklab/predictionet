	Journal-Full = {BMC bioinformatics},
	Mesh = {Breast Neoplasms; Computational Biology; Female; Gene Expression Profiling; Genetic Predisposition to Disease; Humans; Prognosis; Signal Transduction; Tumor Markers, Biological},
	Pages = {1},
	Pmc = {PMC2837025},
	Pmid = {20043860},
	Pst = {epublish},
	Title = {Detection of gene pathways with predictive power for breast cancer prognosis},
	Volume = {11},
