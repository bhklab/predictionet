	Journal = {J Clin Oncol},
	Number = {12},
	Pages = {2284-2293},
	Title = {{Gene Expression Profiles Predict Complete Pathologic Response to Neoadjuvant Paclitaxel and Fluorouracil, Doxorubicin, and Cyclophosphamide Chemotherapy in Breast Cancer}},
