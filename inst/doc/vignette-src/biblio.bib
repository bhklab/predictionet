	Eprint = {http://jnci.oxfordjournals.org/cgi/reprint/jnci;97/12/927.pdf},
	Journal = {J. Natl. Cancer Inst.},
	Number = {12},
	Pages = {927-930},
	Title = {{Limits of Predictive Models Using Microarray Data for Breast Cancer Clinical Treatment Outcome}},
