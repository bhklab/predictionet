		Doi = {10.1158/1078-0432.CCR-07-4756},
		Eprint = {http://clincancerres.aacrjournals.org/cgi/reprint/14/16/5158.pdf},
		Journal = {Clin Cancer Res},
		Number = {16},
		Pages = {5158-5165},
		Title = {{Biological Processes Associated with Breast Cancer Clinical Outcome Depend on the Molecular Subtypes}},
		Url = {http://clincancerres.aacrjournals.org/cgi/content/abstract/14/16/5158},
		Volume = {14},
