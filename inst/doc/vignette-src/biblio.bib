		Doi = {10.1093/jnci/djj052},
		Eprint = {http://jnci.oxfordjournals.org/cgi/reprint/jnci;98/4/262.pdf},
		Journal = {J. Natl. Cancer Inst.},
		Number = {4},
		Pages = {262-272},
		Title = {{Gene Expression Profiling in Breast Cancer: Understanding the Molecular Basis of Histologic Grade To Improve Prognosis}},
		Url = {http://jnci.oxfordjournals.org/cgi/content/abstract/jnci;98/4/262},
		Volume = {98},
		Year = {2006},
		Bdsk-Url-1 = {http://jnci.oxfordjournals.org/cgi/content/abstract/jnci;98/4/262},
