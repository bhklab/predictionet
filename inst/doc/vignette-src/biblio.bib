	Doi = {10.1093/nar/gni179},
	Eprint = {http://nar.oxfordjournals.org/cgi/reprint/33/20/e175.pdf},
	Journal = {Nucl. Acids Res.},
	Number = {20},
	Pages = {e175-},
	Title = {{Evolving gene/transcript definitions significantly alter the interpretation of GeneChip data}},
	Url = {http://nar.oxfordjournals.org/cgi/content/abstract/33/20/e175},
	Volume = {33},
