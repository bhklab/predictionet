	Journal = {J. Natl. Cancer Inst.},
	Number = {16},
	Pages = {1180-1184},
	Title = {{Reporting Recommendations for Tumor Marker Prognostic Studies (REMARK)}},
	Url = {http://jnci.oxfordjournals.org/cgi/content/abstract/jnci;97/16/1180},
	Volume = {97},
