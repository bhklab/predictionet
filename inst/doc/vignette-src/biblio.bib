	Journal = {Breast Cancer Research},
	Note = {See related letters by Rakha et al., http://breast-cancer-research.com/content/9/6/404 and Kreike et al., http://breast-cancer-research.com/content/9/6/405},
	Number = {5},
	Pages = {R65},
	Pubmedid = {17910759},
	Title = {Gene expression profiling and histopathological characterization of triple-negative/basal-like breast carcinomas},
