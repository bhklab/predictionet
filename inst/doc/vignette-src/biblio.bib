	Journal = {Genome Biology},
	Number = {1},
	Pages = {research0005.1-research0005.10},
	Pubmedid = {11806828},
	Title = {Assessment of the relationship between signal intensities and transcript concentration for Affymetrix GeneChip(R) arrays},
