	Journal = {Clin Cancer Res},
	Number = {4},
	Pages = {1115-1122},
	Title = {{Poor Outcome in Estrogen Receptor-Positive Breast Cancers Predicted by Loss of Plexin B1}},
	Url = {http://clincancerres.aacrjournals.org/cgi/content/abstract/13/4/1115},
	Volume = {13},
	Year = {2007},
	Bdsk-Url-1 = {http://clincancerres.aacrjournals.org/cgi/content/abstract/13/4/1115},
