	Doi = {10.1634/theoncologist.11-8-868},
	Eprint = {http://theoncologist.alphamedpress.org/cgi/reprint/11/8/868.pdf},
	Journal = {Oncologist},
	Number = {8},
	Pages = {868-877},
	Title = {{Molecular Classification of Breast Cancer: Limitations and Potential}},
	Url = {http://theoncologist.alphamedpress.org/cgi/content/abstract/11/8/868},
	Volume = {11},
	Year = {2006},
	Bdsk-Url-1 = {http://theoncologist.alphamedpress.org/cgi/content/abstract/11/8/868},
