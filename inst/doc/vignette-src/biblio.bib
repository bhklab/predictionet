	Journal = {J Clin Oncol},
	Number = {19},
	Pages = {3032-3038},
	Title = {{HER2 Testing by Local, Central, and Reference Laboratories in Specimens From the North Central Cancer Treatment Group N9831 Intergroup Adjuvant Trial}},
