	Mesh = {Cell Differentiation; Cells, Cultured; Epithelial Cells; Female; Humans; Mammary Glands, Human; Proto-Oncogene Proteins; Receptor, Notch1; Receptors, Cell Surface; Receptors, Notch; Signal Transduction; Stem Cells; Transcription Factors},
	Number = {6},
	Pages = {R605-15},
	Pmc = {PMC1064073},
