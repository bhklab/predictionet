		Journal = {Blood},
		Number = {21},
		Pages = {5237-5245},
		Title = {{microRNA-29c and microRNA-223 down-regulation has in vivo significance in chronic lymphocytic leukemia and improves disease risk stratification}},
		Url = {http://bloodjournal.hematologylibrary.org/cgi/content/abstract/bloodjournal;113/21/5237},
		Volume = {113},
		Year = {2009},
		Bdsk-Url-1 = {http://bloodjournal.hematologylibrary.org/cgi/content/abstract/bloodjournal;113/21/5237},
