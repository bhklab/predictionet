	Doi = {10.1186/bcr2468},
	Journal = {Breast Cancer Res},
	Journal-Full = {Breast cancer research : BCR},
	Mesh = {Algorithms; Area Under Curve; Breast Neoplasms; Female; Gene Expression Profiling; Humans; Receptors, Estrogen; Sample Size},
