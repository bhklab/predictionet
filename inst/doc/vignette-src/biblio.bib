	Journal = {J Clin Oncol},
	Number = {33},
	Pages = {8331-8339},
	Title = {{Nomograms to Predict Pathologic Complete Response and Metastasis-Free Survival After Preoperative Chemotherapy for Breast Cancer}},
	Url = {http://jco.ascopubs.org/cgi/content/abstract/23/33/8331},
	Volume = {23},
	Year = {2005},
