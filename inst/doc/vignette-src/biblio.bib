	Title = {{Molecular Characterization of Breast Cancer with High-Resolution Oligonucleotide Comparative Genomic Hybridization Array}},
	Url = {http://clincancerres.aacrjournals.org/cgi/content/abstract/15/2/441},
	Volume = {15},
